.data
str1 string "abcdefghijklmnopqrstuvwxyzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzz"
str2 string "e"

.code

int 1 0 // reset tty
int 1 2 // clear tty

lda r0 str1
li r2 0xf100 // start of tty memory

start:
lw r1 r0 0
sw r1 r2 0
addi r0 1
addi r2 1
sub zero r1 zero
jne start

int 1 1

lda r0 str2
li r2 0xf100 // start of tty memory

start2:
lw r1 r0 0
sw r1 r2 0
addi r0 1
addi r2 1
sub zero r1 zero
jne start2

int 1 1 // print

jmp 0